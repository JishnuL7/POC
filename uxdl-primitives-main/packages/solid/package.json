{
  "name": "@coe/uxdl-solid",
  "version": "0.2.0",
  "license": "UNLICENSED",
  "description": "Procter & Gamble's UXDL Solid JS UI Library",
  "source": "./solid-index.tsx",
  "main": "dist/cjs/uxdl-solid.js",
  "module": "dist/esm/uxdl-solid.js",
  "types": "dist/types/uxdl-solid.d.ts",
  "exports": {
